def notepad(s, commands) :
    L = list(s)
    R = []
    
    for command in commands:
        if command[0] == "P":
            L.append(command[2])
        elif command[0] == "L":
            if len(L) != 0:
                R.append(L.pop())
        elif command[0] == "R":
            if len(R) != 0:
                L.append(R[0])
                R = R[1:]
        elif command[0] == "D":
            if len(L) != 0:
                L.pop();
    total = L + R
    return "".join(total)
